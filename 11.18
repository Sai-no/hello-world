#include "stdio.h"
#include "stdlib.h"
#include "time.h"

int main()
{
	int a[3][4];
	int i,j;

	srand(time(0));

	for(i = 0;i < 3; i++)
		{
			for(j = 0; j < 4; j++)
				{

					a[i][j] = rand();
					printf(" %-5d ",a[i][j]);

				} 
			printf("\n");
	
			}

	

	return 0;
}






